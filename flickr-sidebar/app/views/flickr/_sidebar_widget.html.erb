<div class="container">
  <div class="col-md-12">
    <ul>
      <% photos.each do |p| %>
        <div class="col-md-3">
          <% info = flickr.photos.getInfo(photo_id: p.id) %>
          <%= link_to(image_tag(FlickRaw.url_s(info), class: "flickr_photo", title: p.title), FlickRaw.url_photopage(info)) %>
        </div>
      <% end %>
    </ul>
  </div>
</div>